$('#star').raty({
  readOnly: true,
  score: <%= @card.average_rating %>,
  path: '/assets'
});

$('#user_star').raty({
  score: <%= @readiness.question1 %>,
  path: '/assets',
  click: function(score, evt) {
    $.ajax({
      url: '/readiness/' + <%= @readiness.id %>,
      type: 'PATCH',
      data: { score: score }
    }).done(function (data){});
  }
});